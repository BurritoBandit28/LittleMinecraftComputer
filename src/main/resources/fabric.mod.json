{
  "schemaVersion": 1,
  "id": "lmc",
  "version": "${version}",
  "name": "Little Minecraft Computer",
  "description": "Adds simple computers to Minecraft, using the educational low level language of Little Man Computer",
  "authors": ["BurritoBandit28"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/lmc/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.burritobandit28.lmc.client.LMCClient"
    ],
    "main": [
      "io.github.burritobandit28.lmc.LMC"
    ]
  },
  "mixins": [
    "lmc.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
